  & a:hover, & span.current { border-color: #000; } 

  & span.current { font-weight: bold; } }

#site-social { position: relative; }
.addthis_default_style { position: absolute; right: -1em; top: 268px; }
body.home .addthis_default_style { }

img.alignleft { float: left; margin: 0 1em 1em 0; line-height: 1; }

body.single div.post .entry-content, 
body.page div.page .entry-content { margin-bottom: 3em; }

.byline { 
  margin: 0 0 1em 0; 
  color: #999;
  
  em { display: block; } }
  
.poem { 
  margin: 0 0 1em;
  
  h2 { }
  
  pre { }
  
  cite { } }
  
blockquote { margin: 1.5em 0; }
blockquote cite { display: block; text-align: right; font-sty1le: italic; font-size: 1.3em; opacity: .7; }
blockquote em { display: inline; }

.call-access-menu { 
  background: #F7F7F7 !important;
  padding: 3px 6px 1px;
  position: absolute; 
  font-size: 9px !important; 
  color: #999;
  font-weight: 400; 
  margin-top: -15px; 
  margin-left: 4px;
  opacity: .4;
  text-shadow: 0 0 1px rgba(30,30,30,.5); 
  cursor: pointer; }

@media screen and (max-device-width: 480px) {
  /* iphone only */
  #footer {
    position: relative;
    bottom: 0; 
    left: 0;
    opacity: 1;
    width: 100%;
  }
}

